{
  "name": "division-api",
  "version": "1.0.0",
  "description": "Division API - AI agent orchestration with multi-model routing",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && prisma db push && ts-node prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@prisma/client": "^5.20.0",
    "express": "^4.21.0",
    "uuid": "^10.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.0",
    "vercel": "^50.13.2"
  }
}
